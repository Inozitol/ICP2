@startuml
class Person {
	- string name
    - string surname
    - date birthdate
    ---
    + string GetName ( )
    + string GetWholeName ( )
    + date GetBirthday ( )
    + date GetAge ( )
}
class School {
    - vector<Person> people
    ---
    + Person GetPersonByName ( string )
}
relation Person -- School
actor Alice Person
actor Amanda Person
actor VUT School
message Alice -> Amanda : GetName()
activate Amanda
message Amanda -> VUT : GetPersonByName("Amanda")
message VUT -> Amanda : name
deactivate Amanda
message Amanda -> Alice : name
@enduml
