@startuml
class Person {
	- date birthdate
	- string name
	- string surname
	---
	+ date GetAge (  )
	+ date GetBirthday (  )
	+ string GetName (  )
	+ string GetWholeName (  )
}
class School {
	- vector<Person> people
	---
	+ Person GetPersonByName ( string )
}
relation Person -- School
actor Alice Person
actor Amanda Person
actor VUT School
message Alice -> Amanda : GetName()
activate Amanda
message Amanda -> VUT : GetPersonByName("Amanda")
return VUT -> Amanda : string pizza
deactivate Amanda
return Amanda -> Alice : string name

@enduml